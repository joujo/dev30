

{% extends "EshopStockBundle:Default:index.html.twig" %}
    {% block container %}
        <div class="page-content">
        <div class="page-header position-relative">
            <h1>
                Tables
                <small>
                    <i class="icon-double-angle-right"></i>
                    Les Stocks
                </small>
            </h1>
        </div><!--/.page-header-->

        <div class="row-fluid">
        <div class="span12">
        <!--PAGE CONTENT BEGINS-->

        <div class="row-fluid">
        <div class="span12">
        <table id="sample-table-1" class="table table-striped table-bordered table-hover">
            <thead>
            <tr>

                <th>id</th>
                <th>quantite</th>
                <th>supprimer</th>
                <th>modifier</th>





            </thead>

            <tbody>
            {% for stock in stocks %}
            <tr>




                <td>{{ stock.id }}</td>

                <td>{{ stock.quantite }}</td>
                <td><a href="{{ path("eshop_stock_deletestok",{'id': stock.id}) }}" class="btn btn-mini btn-danger">  <i class="icon-trash bigger-120"></i> </a>
                <td><a href="{{ path("eshop_stock_updatestok",{'id': stock.id}) }}" class="btn btn-mini btn-info">  <i class="icon-edit bigger-120"></i> </a>

                    </td> {% endfor %}
            </tr>

            <tr>





                <div class="hidden-desktop visible-phone">
                    <div class="inline position-relative">
                        <button class="btn btn-minier btn-primary dropdown-toggle" data-toggle="dropdown">
                            <i class="icon-cog icon-only bigger-110"></i>
                        </button>

                        <ul class="dropdown-menu dropdown-icon-only dropdown-yellow pull-right dropdown-caret dropdown-close">
                            <li>
                                <a href="#" class="tooltip-info" data-rel="tooltip" title="View">
																		<span class="blue">
																			<i class="icon-zoom-in bigger-120"></i>
																		</span>
                                </a>
                            </li>

                            <li>
                                <a href="#" class="tooltip-success" data-rel="tooltip" title="Edit">
																		<span class="green">
																			<i class="icon-edit bigger-120"></i>
																		</span>
                                </a>
                            </li>

                            <li>
                                <a href="#" class="tooltip-error" data-rel="tooltip" title="Delete">
																		<span class="red">
																			<i class="icon-trash bigger-120"></i>
																		</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                </td>
            </tr>






            </tbody>
        </table>
    {% endblock %}